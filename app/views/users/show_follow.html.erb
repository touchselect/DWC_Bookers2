<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h2><%= @title %> Users</h2>
			<table class="table">
				<tr>
				<th>image</th>
				<th>name</th>
				<th></th>
				<th></th>
				<th></th>
				</tr>
				<% @users.each do |user| %>
					<tr>
						<td>
							<%= image_tag user.get_profile_image(100,100) %>
						</td>
						<td>
							<%= user.name %>
						</td>
						<%= render 'follow', user: user %>
						<td>
							<%= link_to 'Show', user_path(user.id)%>
						</td>
					</tr>
				<% end %>
			</table>
		</div>
	</div>
</div>